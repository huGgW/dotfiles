{
	"theme": "GitHub",
	"selectedAuthType": "oauth-personal",
	"mcpServers": {
		"sequential-thinking": {
			"command": "npx",
			"args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
		},
		"fetcher": {
			"command": "npx",
			"args": ["-y", "fetcher-mcp"]
		},
		"context7": {
			"command": "npx",
			"args": ["-y", "@upstash/context7-mcp"]
		},
		"grep": {
			"url": "https://mcp.grep.app"
		},
		"notionMCP": {
			"command": "npx",
			"args": ["-y", "mcp-remote", "https://mcp.notion.com/mcp"]
		}
	}
}
